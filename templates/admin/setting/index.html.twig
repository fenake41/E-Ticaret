{% extends 'admin/adminbase.html.twig' %}



{% block body %}


                    <div class="panel-body">
                        <div class="table-responsive">

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>Description</th>
                <th>Keywords</th>
                <th>Company</th>
                <th>Address</th>
                <th>Fax</th>
                <th>Phone</th>
                <th>Email</th>

            </tr>
        </thead>
        <tbody>
        {% for setting in settings %}
            <tr>
                <td>{{ setting.id }}</td>
                <td>{{ setting.title }}</td>
                <td>{{ setting.description }}</td>
                <td>{{ setting.keywords }}</td>
                <td>{{ setting.company }}</td>
                <td>{{ setting.address }}</td>
                <td>{{ setting.fax }}</td>
                <td>{{ setting.phone }}</td>
                <td>{{ setting.email }}</td>


                <td>
                    <a href="{{ path('admin_setting_show', {'id': setting.id}) }}">show</a>
                    <a href="{{ path('admin_setting_edit', {'id': setting.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="18">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('admin_setting_new') }}">Create new</a>
            </div>
        </div>


{% endblock %}
